{
    "CodeGPT.model": "gpt-4",
    "CodeGPT.query.language": "Portuguese",
    "editor.wordWrapColumn": 80,
    "editor.minimap.enabled": false,
    "editor.formatOnSave": true,
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/node_modules": true
        },
        "[python]": {
            "editor.defaultFormatter": "ms-python.black-formatter"
        },
        "python.formatting.provider": "none"
}